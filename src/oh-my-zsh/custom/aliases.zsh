# general
alias reload='source ~/.zshrc'

# git
alias git='hub'
alias g='git'
alias gs='git status'
alias gadd='git add'
alias gpull='git pull'

# ls aliases
alias ls='gls --indicator-style=slash --color=auto --group-directories-first'
alias ll='ls -l -v --almost-all --human-readable'
alias lr='ll -R'
